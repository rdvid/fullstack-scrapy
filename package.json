{
  "name": "mscrappyapi",
  "version": "1.0.2",
  "description": "REST Api for data scrapping",
  "main": "index.js",
  "scripts": {
    "start": "ts-node src/index.ts",
    "dev": "nodemon",
    "build": "npx tsc",
    "serve": "parcel frontend/src/index.html --dist-dir=frontend/dist --cache-dir=frontend/.prod/.parcel-cache",
    "serve:dev": "parcel serve frontend/src/index.html --dist-dir=frontend/dist --cache-dir=frontend/.dev/.parcel-cache",
    "lint": "eslint . --ext .ts",
    "lint-fix": "eslint . --ext .ts --fix",
    "test": "jest",
    "test:coverage": "jest --coverage --coverageReporters text-summary",
    "e2e": "playwright test --reporter=html",
    "e2e:report": "playwright show-report"
  },
  "keywords": [
    "api",
    "rest",
    "express",
    "typescript",
    "jest"
  ],
  "author": "Rafael David",
  "license": "ISC",
  "devDependencies": {
    "@playwright/test": "^1.41.2",
    "@types/axios": "^0.14.0",
    "@types/babel__core": "^7.20.5",
    "@types/cheerio": "^0.22.35",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.13",
    "@types/ioredis": "^4.28.10",
    "@types/jest": "^29.5.12",
    "@types/node": "^18.0.3",
    "@types/node-cron": "^3.0.11",
    "@typescript-eslint/eslint-plugin": "^5.30.6",
    "@typescript-eslint/parser": "^5.30.6",
    "eslint": "^8.19.0",
    "nodemon": "^3.1.0",
    "parcel": "^2.11.0",
    "rimraf": "^5.0.5",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "axios": "^1.6.7",
    "cheerio": "^1.0.0-rc.12",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "ioredis": "^5.3.2",
    "jest": "^29.7.0",
    "node-cron": "^3.0.3",
    "ts-node": "^10.9.2"
  }
}
